{
  "name": "template-angular",
  "version": "0.1.0",
  "description": "Basic Angular + Express Template using TypeScript",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "pretsc": "typings install typedi=file:node_modules/typedi/typings.json",
    "tsc": "tsc",
    "pretypetest": "typings install typedi=file:node_modules/typedi/typings.json",
    "typetest": "tsc && node dist/typedi-test/app.js",
    "prestart": "npm install && typings install typedi=file:node_modules/typedi/typings.json",
    "start": "tsc  && webpack --config webpack.config.js && node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
