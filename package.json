    "@angular/platform-browser": "^2.4.5",
    "@angular/platform-browser-dynamic": "^2.4.5",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.5",
    "es6-shim": "^0.35.3",
    "express": "^4.14.0",
    "mongodb": "^2.2.22",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.3",
    "systemjs": "^0.20.2",
